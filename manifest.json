{
  "manifest_version": 3,
  "name": "Codeforces Problem Suggester",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_title": "Codeforces Problem Suggester"
  },
  "host_permissions": [
    "https://codeforces.com/*"
  ],
  "icons": {
    "48": "icon.png"
  }
}
